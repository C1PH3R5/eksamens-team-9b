<Window x:Class="nida.tools_team_9b.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:nida.tools_team_9b"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStyle="None"
        Title="MainWindow" Height="800" Width="1200" MaxHeight="800" MaxWidth="1200">
    <Grid Background="{StaticResource ClrGridBackground}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="1000"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Frame Grid.Column="0"  Name="menuHolder"  Width="200" Height="auto" HorizontalAlignment="Left" NavigationUIVisibility="Hidden"  Source="/View/Menu/AdminMenu.xaml"/>
        <Frame Grid.Column="1" Name="pageHolder" Width="1000" Height="auto"  NavigationUIVisibility="Hidden" HorizontalAlignment="Right"  Source="/View/page/MedarbejderList.xaml"/>

    </Grid>


</Window>
